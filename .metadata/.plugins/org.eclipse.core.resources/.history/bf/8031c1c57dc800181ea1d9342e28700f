import java.io.*;
public class CH02_14
{
	public static void main(String[] args) throws IOException
	{
		String str1;
		BufferedReader keyin_str = new BufferedReader(new InputStreamReader(System.in));
		
		System.out.print("鍵入字串1：");
		keyin_str.readLine();
		System.out.println("顯示字串1："+str1);
		
		System.out.print("鍵入字串2：");
		keyin_str.readLine();
		System.out.println("顯示字串2："+str1);
	}
}
